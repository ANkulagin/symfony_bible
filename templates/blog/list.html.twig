{# templates/blog/list.html.twig #}
<!DOCTYPE html>
<html>
<head>
    <title>Blog List</title>
</head>
<body>
<h1>Blog Posts</h1>
<ul>
    {# Допустим, у нас есть массив постов #}
    {% for post in posts %}
        <li>
            <a href="{{ path('blog_show', {'id': post.id}) }}">{{ post.title }}</a>
        </li>
    {% endfor %}
</ul>
</body>
</html>
